    {
        "identifier": "jenkinsJob",
        "description": "This blueprint represents a job event from Jenkins",
        "title": "Jenkins Job",
        "icon": "Jenkins",
        "schema": {
            "properties": {
                "jobName": {
                    "type": "string",
                    "title": "Job Name"
                },
                "jobStatus": {
                    "type": "string",
