                    "enum": [
                        "created",
                        "unknown",
                        "passing",
                        "failing"
                    ],
                    "enumColors": {
                        "passing": "green",
                        "created": "darkGray",
                        "failing": "red",
                        "unknown": "orange"
                    }
                },
                "timestamp": {
                    "type": "string",
                    "format": "date-time",
                    "title": "Timestamp",
                    "description": "Last updated timestamp of the job"
                },
                "url": {
                    "type": "string",
                    "title": "Project URL"
                },
                "parentJob": {
                    "type": "object",
                    "title": "Parent Job"
                }
            },
            "required": []
        },
        "mirrorProperties": {},
        "calculationProperties": {},
        "relations": {}
    },
